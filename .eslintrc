{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/essential",
    "prettier",
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "vue/script-setup-uses-vars": 0,
    "prefer-arrow-callback": "off",
    "comma-dangle": 0,
    "linebreak-style": 0,
    "no-unused-vars": "warn",
    "prefer-spread": "off",
    "arrow-parens": "off",
    "no-restricted-syntax": "off",
    "no-plusplus": 0,
    "array-callback-return": 2,
    "block-scoped-var": 2,
    "curly": 2,
    "eqeqeq": 2,
    "no-alert": 1,
    "no-else-return": 1,
    "no-empty-function": 1,
    "no-use-before-define": ["error", { "variables": true }],
    "class-methods-use-this": "off",
    "no-nested-ternary": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "no-unused-expressions": [
      2,
      {
        "allowTernary": true
      }
    ]
  }
}
